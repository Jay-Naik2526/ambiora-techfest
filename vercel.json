{
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/src/(.*)",
            "dest": "/src/$1"
        },
        {
            "src": "/(.*).html",
            "dest": "/$1.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1.html"
        }
    ]
}